{
    "name": "start",
    "version": "0.0.1",
    "description": "A way to start web applications",
    "private": true,
    "license": "MIT",
    "author": "@davi-mbatista",
    "scripts": {
        "start": "webpack serve --config webpack/start.js",
        "build": "webpack --config webpack/build.js"
    },
    "devDependencies": {
        "@babel/cli": "7.12.16",
        "@babel/core": "7.12.16",
        "@babel/preset-env": "7.12.16",
        "autoprefixer": "10.2.4",
        "babel-loader": "8.2.2",
        "clean-webpack-plugin": "3.0.0",
        "css-loader": "5.0.2",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "5.1.0",
        "mini-css-extract-plugin": "1.3.7",
        "postcss": "8.2.6",
        "postcss-cli": "8.3.1",
        "postcss-import": "14.0.0",
        "postcss-loader": "5.0.0",
        "postcss-preset-env": "6.7.0",
        "prettier": "2.2.1",
        "purgecss-webpack-plugin": "4.0.0",
        "size-plugin": "2.0.1",
        "style-loader": "2.0.0",
        "svelte-loader": "3.0.0",
        "svelte-preprocess": "4.6.9",
        "tailwindcss": "2.0.3",
        "webpack": "5.22.0",
        "webpack-cli": "4.5.0",
        "webpack-dev-server": "3.11.2",
        "webpack-merge": "5.7.3"
    },
    "dependencies": {
        "svelte": "3.32.3"
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "browserslist": [
        "last 3 chrome version",
        "last 3 firefox version",
        "last 3 safari version",
        "last 3 ios_saf version"
    ],
    "postcss": {
        "plugins": {
            "postcss-preset-env": {},
            "postcss-import": {},
            "tailwindcss": {}
        }
    },
    "prettier": {
        "printWidth": 80,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "none",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "arrowParens": "avoid"
    }
}
